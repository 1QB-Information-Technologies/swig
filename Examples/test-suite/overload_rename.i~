%module overload_rename


%inline %{

class Base {
public:
	Base(){}
	virtual void Print(){printf("I'm Base.\n");}
	virtual ~Base(){}
};

class Derived : public Base {
public:
	Derived(){}
	void Print(){printf("I'm Derived.\n");}
	virtual ~Derived(){}
};



void test(Base* base){
	base->Print();
}
void test(int hello){
	printf("%d\n", hello);
}

%}

